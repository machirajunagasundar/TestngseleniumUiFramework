{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "array",
  "title": "The Root Schema",
  "items": {
    "$id": "#/items",
    "type": "object",
    "title": "The Items Schema",
    "required": [
      "applicationErrorCode",
      "applicationErrorMessage"
    ],
    "properties": {
      "applicationErrorCode": {
        "$id": "#/items/properties/applicationErrorCode",
        "type": "integer",
        "title": "The Applicationerrorcode Schema",
        "default": 0,
        "examples": [
          4003
        ]
      },
      "applicationErrorMessage": {
        "$id": "#/items/properties/applicationErrorMessage",
        "type": "string",
        "title": "The Applicationerrormessage Schema",
        "default": "",
        "examples": [
          "rishubh is an invalid FQDN."
        ],
        "pattern": "^(.*)$"
      }
    }
  }
}